{
  "expo": {
    "name": "IVFTrackerApp",
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermission": "We need to access your camera to scan documents."
        }
      ],
      "react-native-vision-camera"
    ],
    "scheme": "espouse",
    "deepLinking": true,
    "slug": "IVFTrackerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.ivftrackerapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.ivftrackerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}